  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "@material-ui/core": "^4.3.3",
    "axios": "^0.18.0",
    "express": "^4.16.3",
    "if-env": "^1.0.4",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.26",
    "mysql": "^2.17.1",
    "mysql2": "^1.6.5",
    "react-calendar": "^2.19.1",
    "react-moment": "^0.9.2",
    "sequelize": "^5.16.0"
  }
}
